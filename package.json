{
  "name": "vylbot-app",
  "version": "3.2.2",
  "description": "A discord bot made for Vylpes' Den",
  "main": "./dist/vylbot",
  "typings": "./dist",
  "scripts": {
    "clean": "rm -rf node_modules/ dist/",
    "build": "tsc",
    "start": "node ./dist/vylbot",
    "test": "jest . --passWithNoTests",
    "db:up": "typeorm migration:run -d dist/database/dataSources/appDataSource.js",
    "db:down": "typeorm migration:revert -d dist/database/dataSources/appDataSource.js",
    "db:create": "typeorm migration:create ./src/database/migrations",
    "release": "np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vylpes/vylbot-app"
  },
  "author": "Vylpes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vylpes/vylbot-app/issues",
    "email": "helpdesk@vylpes.com"
  },
  "homepage": "https://github.com/Vylpes/vylbot-app",
  "funding": "https://ko-fi.com/vylpes",
  "dependencies": {
    "@discordjs/rest": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.0",
    "emoji-regex": "^10.0.0",
    "minimatch": "9.0.5",
    "mysql": "^2.18.1",
    "random-bunny": "^2.1.6",
    "typeorm": "0.3.20"
  },
  "resolutions": {
    "**/semver": "^7.5.2",
    "**/undici": "^6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "np": "^10.0.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.0.0"
  }
}
